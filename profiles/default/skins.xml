<?xml version="1.0"?>
<!-- This file holds the setup configuration for the portal_skins tool -->
<object name="portal_skins" allow_any="False" cookie_persistence="False"
        default_skin="sleicTheme" request_varname="plone_skin">

	<object name="sleicTheme"
		meta_type="Filesystem Directory View"
		directory="Products.sleicTheme:skins/sleicTheme"/>
	
	<skin-path name="sleicTheme" based-on="Plone Default">
		<layer name="sleicTheme"
			 insert-after="custom"/>
	</skin-path>
</object>